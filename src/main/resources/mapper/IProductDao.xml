<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.why.dao.IProductDao">
    <select id="findProductCount" resultType="int">
        select count(1)
        from t_product
    </select>

    <select id="findProductRemark" resultType="String">
        select remark
        from t_product
        where id = 1
    </select>
</mapper>